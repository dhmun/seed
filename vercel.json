{
  "version": 2,
  "name": "hope-seed-campaign",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SITE_URL": "https://hope-seed-campaign.vercel.app"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}